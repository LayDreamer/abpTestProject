@page
@using LocalTest.Localization
@using LocalTest.Web.Pages.MaterialSpecificationList
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<LocalTestResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="ListPage" asp-page="/MaterialSpecificationList/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewInventory"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="UploadFileDto.File"></abp-input>
            @*<abp-input asp-for="ListDto.ProjectCode"></abp-input>*@
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
